        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Submit Button Disable for Leave App</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading) {
        return;
    }

    var leaveDuration = g_form.getValue('leave_duration');

    if (!leaveDuration) {
        //g_form.addErrorMessage('Leave duration is required.');
        g_form.setSubmitButtonDisabled(true); // Disable Submit button
    } else {
        //g_form.clearMessages();
        g_form.setSubmitButtonDisabled(false); // Enable Submit button if valid
    }
}
]]></script>
